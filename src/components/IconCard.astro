---
import { Icon } from 'astro-icon/components';
const { set, name } = Astro.props;
---
<div class="w-[140px] py-5 bg-white border border-gray-200 rounded-lg shadow-sm dark:bg-gray-700 dark:border-gray-700">
    <div class="flex flex-col items-center">
        <Icon name=`${set}:${name}` class="w-12 h-12 mb-3 bg-transparent" />
        <h5 class="mb-1 text-xl font-medium text-gray-900 dark:text-white">{name}</h5>
    </div>
</div>